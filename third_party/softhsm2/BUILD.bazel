load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_binary")

package(default_visibility = ["//visibility:public"])

go_library(
    name = "test_config",
    importpath = "github.com/lowRISC/ot-provisioning/third_party/softhsm2/test_config",
    srcs = ["test_config.go"],
)

go_binary(
    name = "softhsm2",
    srcs = ["softhsm2.go"],
    data = ["@softhsm2//:softhsm2"],
    deps = [
        ":test_config",
        "@io_bazel_rules_go//go/tools/bazel",
    ],
)
